<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorPrimaryDark</item>
        <item name="windowNoTitle">true</item>
        <item name="android:textColorPrimary">@android:color/background_light</item>
    </style>

    <style name="AppTheme.AppDate" parent="android:Theme.Holo.Light.NoActionBar">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <!--设置dialog不全屏 -->
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowNoTitle">true</item>
        <!--除去title，如果在代码里面有其他设置会没效果的不过完全没效果-->
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:backgroundDimEnabled">false</item>
        <!--<item name="android:windowBackground">@null</item><!–除去背景色–>-->
    </style>
    <style name="Theme.Picker" parent="Theme.AppCompat.Light">
        <item name="android:editTextStyle">@style/Widget.EditText.White</item>
    </style>
    <style name="MyDatePicker" parent="@android:style/Widget.Holo.DatePicker">
        <item name="android:textColorPrimary">@android:color/white</item>
    </style>
    <style name="Widget.EditText.White" parent="@android:style/Widget.EditText">
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">#00ff00</item>
    </style>
    <style name="Widget.SeekBar.Normal" parent="@android:style/Widget.SeekBar">
        <item name="android:maxHeight">2.5dip</item>
        <item name="android:indeterminateOnly">false</item>
        <item name="android:screenOrientation">portrait</item>
        <item name="android:indeterminateDrawable">
            @android:drawable/progress_indeterminate_horizontal
        </item>
        <item name="android:progressDrawable">@drawable/seekbar</item>
        <item name="android:minHeight">2.5dip</item>
        <item name="android:thumb">@drawable/seekbar_thumb</item>
        <item name="android:thumbOffset">1.0dip</item>

    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="NoTitleTranslucentTheme" parent="AppTheme">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>
    <!--横向分割线-->
    <style name="view_h01">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0.1dp</item>
        <item name="android:background">#e5e5e5</item>
    </style>

    <style name="view_h1">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">#e5e5e5</item>
    </style>

    <style name="view_h05">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0.5dp</item>
        <item name="android:background">#e5e5e5</item>
    </style>
    <!--纵向分割线-->
    <style name="view_v01">
        <item name="android:layout_width">0.1dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">#e5e5e5</item>
    </style>
    <!--取样填报-->
    <style name="yjcztbtv1">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:gravity">center_vertical|left</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/color_4d4d4d</item>
        <item name="android:layout_weight">1</item>
        <item name="android:singleLine">true</item>
    </style>
    <!--取样填报-->
    <style name="yjcztbtv2">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:gravity">center_vertical|left</item>
        <item name="android:paddingLeft">10sp</item>
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">#5b5b5b</item>
        <item name="android:layout_weight">3</item>
        <item name="android:singleLine">true</item>
    </style>
    <!--拍照Dialog-->
    <style name="customDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowBackground">@android:color/background_light</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="xiangceStyle" parent="Theme.AppCompat.Light.DarkActionBar"></style>
    <!--应急填报故障单选Dialog-->
    <style name="AlertDialogCustom">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/colorPrimaryDark</item>
    </style>
    <!--功能pop-->
    <style name="popwin_slide_style" mce_bogus="1" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/push_bottom_in</item>
        <item name="android:windowExitAnimation">@anim/push_bottom_out</item>
    </style>
    <!--图例侧滑菜单栏条目字体-->
    <style name="lfmTexttitle">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">13sp</item>
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:layout_height">35dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:singleLine">true</item>
    </style>
    <!--巡检上报ll-->
    <style name="reportll">
        <item name="android:layout_height">35dp</item>
        <item name="android:paddingRight">@dimen/dp10</item>
        <item name="android:paddingLeft">@dimen/dp10</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:gravity">center_vertical</item>
    </style>    <!--巡检上报字体left-->
    <style name="reportTextleft">
        <item name="android:textColor">#333333</item>
        <item name="android:textSize">14sp</item>
        <item name="android:layout_marginRight">@dimen/dp10</item>
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:singleLine">true</item>
    </style> <!--巡检上报字体right-->
    <style name="reportTextright">
        <item name="android:textColor">#333333</item>
        <item name="android:textSize">14sp</item>


        <item name="android:gravity">left|center_vertical</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:singleLine">true</item>
    </style>

    <!--<item name="android:layout_marginTop">@dimen/dp15</item>-->
    <!--图例ll-->
    <style name="view_tulil">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dp50</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:paddingLeft">@dimen/dp10</item>
    </style>
    <!--图例tv-->
    <style name="view_tuliltv">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:paddingLeft">@dimen/dp10</item>
        <item name="android:background">@color/transparent</item>

        <item name="android:textColor">#33363e</item>
        <item name="android:textSize">@dimen/sp15</item>
    </style>
    <!--下啦功能pop-->
    <style name="AnimationTopFade" mce_bogus="1" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/dropdown_in</item>
        <item name="android:windowExitAnimation">@anim/dropdown_out</item>
    </style>
    <!--图层控制-->
    <style name="layercontrol_cbox">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_marginRight">12dp</item>
        <item name="android:padding">8dp</item>
        <item name="android:background">@drawable/view_legend_checkbg</item>
        <item name="android:button">@null</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingBottom">8dp</item>
        <item name="android:textColor">@color/checkbox_tv</item>
        <item name="android:textSize">13dp</item>
    </style>
    <!--侧滑图例文字-->
    <style name="legendtext">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:layout_weight">1</item>
        <item name="android:singleLine">true</item>
    </style>
    <!--图层控制radio-->
    <style name="layercontrol_rb">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">@android:color/background_light</item>
        <item name="android:button">@null</item>
        <item name="android:drawablePadding">5dp</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:paddingBottom">8dp</item>
        <item name="android:textColor">@color/radio_text_color</item>
        <item name="android:textSize">14sp</item>
    </style>
    <!--任务详情LlorRl-->
    <style name="taskdetails_ll">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dp40</item>
        <item name="android:paddingLeft">@dimen/dp10</item>
        <item name="android:paddingRight">@dimen/dp10</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:background">@color/white</item>
    </style>

    <style name="taskdetails_tvleft">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textSize">@dimen/sp14</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:textColor">#666666</item>
    </style>

    <style name="taskdetails_tvright">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textSize">@dimen/sp14</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:textColor">#333333</item>
    </style>

    <style name="view_v1">
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">#e5e5e5</item>
    </style>

    <!--监测详情ll-->
    <style name="ll_jcdetail">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_weight">1</item>

        <item name="android:layout_height">25dp</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:orientation">horizontal</item>
    </style>
    <!--监测详情texttitileleft-->
    <style name="tvl_jcdetail">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#666666</item>
        <item name="android:singleLine">true</item>
    </style>
    <!--监测详情texttitileright-->
    <style name="tvr_jcdetail">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#333333</item>
        <item name="android:paddingLeft">7dp</item>
        <item name="android:singleLine">true</item>
    </style>
    <!--监测详情rb样式-->
    <style name="rb_jcdetail">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">@dimen/dp40</item>
        <item name="android:layout_weight">1</item>
        <item name="android:button">@null</item>
        <item name="android:drawablePadding">@dimen/dp4</item>
        <item name="android:drawableBottom">@drawable/selectot_gbbg_jcclc</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/btn_login_bg</item>
        <item name="android:textSize">@dimen/sp14</item>
        <item name="android:singleLine">true</item>
    </style>
</resources>
